print "I will now count my chickens:"
# prints text
print "Henz", 25 + 30 / 6 # counts henz ad prints
print "Roosters", 100 - 25 * 3 % 4 # counts roosters and prints 

print "Now I will count the eggs:" # prints

print 3 + 2 + 1 - 5 + 4 % 2 - 1 / 4 + 6 # calculates

print "Is it true that 3 + 2 < 5 - 7?" # prints question

print 3 + 2 < 5 - 7 # calculates question above

print "What is 3 + 2?", 3 + 2 # prints question and calculates
print "What is 5 - 7?", 5 - 7  # prints question and calculates

print "Oh, that's why it's False." # prints

print "How about some more." # prints

print "Is it greater?", 5 > -2  # prints question, not calculations
print "Is it greater or equal?", 5 >= -2 # prints question, not calculations
print "Is it less or equal?", 5 <= -2 # prints question, not calculations
