#Oppgave 1 | Printer ut fugl naar man kalle fuksjon.
def ascii_fugl():
    print """ 
                  \/_ 
             \,   /( ,/
              \\\' ///
               \_ /_/
               (./
                '`
    """
#oppgave 2 | Finner bitAnd av x og y
def bitAnd(x, y):
    print bitAnd()
#Oppgave 4 | Finner bitXor av x og y
def bitXor (x, y):
	return x^y
#Oppgave 5 | Finner bitOr av x og y
def bitOr (x, y):
	return x|y
#Oppgave 6 | Finner 
def ascii8Bin (A):
	ord('A')
	A = '{0:08b}'.format(2)
	return A

